<!-- GET DOGEC MASTERNODE INFO DAILY -->
<?php

$url = 'https://masternodes.online/currencies/DOGEC/';

$content = file_get_contents($url);
$first_step = explode( '<div class="panel-body">' , $content );
$masternodedailyinfo = explode("</div>" , $first_step[1] );

// echo $masternodedailyinfo[0];

?>
<!-- Ping Method -->

<?php

$masternode = "DOGECMN01";
$host = 'xx.xx.xx.xxx'; 	//IP of MASTERNODE
$port = xxxx; 				// Port Number
$waitTimeoutInSeconds = 1; 
if($fp = fsockopen($host,$port,$errCode,$errStr,$waitTimeoutInSeconds)){  
   echo "<img style='display:block; width:32px;height:32px;' id='DOGEC'                 
       src='data:image/webp;base64,' />";
   echo $masternode."<br>".'<b><font color="green">MasterNode is ONLINE !</font></b><br>';
   echo $masternodedailyinfo[0];
   
} else {
    echo $masternode." ".'<br><b><font color="red">MasterNode is OFFLINE !</font></b><br>';
    // Send Discord Message to Channel
    $curl =curl_init("https://discordapp.com/api/webhooks/0000000000000/TH1S1S$AMPL3C0DE0FD!$C0RDW3bh0OkS");
    curl_setopt($curl, CURLOPT_POST, 1);
    curl_setopt($curl, CURLOPT_POSTFIELDS, json_encode(array("content" => ":warning:  DOGECMN01 is OFFLINE  :warning:", "username" => "DOGECMN01")));
    echo curl_exec($curl);
		}
fclose($fp);

?>

